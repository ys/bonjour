    "@downwindcss/text-decoration": "^1.1.1",
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/jit": "^0.1.18",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.2.5",
