{
  "build": {
    "env": {
      "HUGO_VERSION": "0.76.2"
    }
  },
  "redirects": [
    { "source": "/index.xml", "destination": "/feed.json" }
  ],
  "headers": [
    {
      "source": "(.*).(jpg|jpeg|gif|png)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "public,max-age=31536000,immutable"
      }]
    }
  ]
}
