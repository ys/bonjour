{
  "redirects": [
    {
      "source": "/portfolio",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/shop",
      "destination": "https://shop.yannickschutz.com",
      "permanent": true
    },
    {
      "source": "/shop/:slug",
      "destination": "https://shop.yannickschutz.com/products/:slug",
      "permanent": true
    },
    {
      "source": "/shop/",
      "destination": "https://shop.yannickschutz.com",
      "permanent": true
    },
    {
      "source": "/shop/:slug/",
      "destination": "https://shop.yannickschutz.com/products/:slug",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "(.*).(webp|jpg|jpeg|gif|png|woff2|woff)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    }
  ]
}
