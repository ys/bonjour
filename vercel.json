{
  "redirects": [
    {
      "source": "/portfolio",
      "destination": "/",
      "permanent": true,
    }
  ],
  "headers": [
    {
      "source": "(.*).(webp|jpg|jpeg|gif|png|woff2|woff)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    }
  ]
}
