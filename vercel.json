{
  "redirects": [
    {
      "source": "/portfolio",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/bonjour",
      "destination": "https://bonjour.yannickschutz.com"
    },
    {
      "source": "/bonjour/:slug",
      "destination": "https://bonjour.yannickschutz.com/:slug"
    }
  ],
  "headers": [
    {
      "source": "(.*).(webp|jpg|jpeg|gif|png|woff2|woff)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    }
  ]
}
