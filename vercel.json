{
  "build": {
    "env": {
      "HUGO_VERSION": "0.76.2"
    }
  },
  "redirects": [
    { "source": "/index.xml", "destination": "/feed.json" }
  ],
  "routes": [
    {
      "src": "(.*).(jpg|jpeg|gif|png)",
      "headers": {
        "Cache-Control": "public,max-age=31536000,immutable"
      }
    }
  ]
}
